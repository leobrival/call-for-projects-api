{
  "name": "call-for-projects-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "db:up": "docker compose up -d",
    "docker:start": "node -e \"process.platform === 'win32' ? require('child_process').spawn('cmd', ['/c', 'scripts/start-docker.cmd'], {stdio: 'inherit'}) : require('child_process').spawn('./scripts/start-docker.sh', [], {stdio: 'inherit'})\"",
    "docker:start:unix": "./scripts/start-docker.sh",
    "docker:start:windows": "scripts/start-docker.cmd",
    "prepare": "husky"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.4",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "1.11.24",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "hot-hook": "^0.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "node-fetch": "^3.3.2",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.8",
    "ts-node-maintained": "^10.9.5",
    "tw-animate-css": "^1.3.3",
    "typescript": "~5.8",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/inertia": "^3.1.1",
    "@adonisjs/lucid": "^21.6.1",
    "@adonisjs/session": "^7.5.1",
    "@adonisjs/shield": "^8.2.0",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^4.0.0",
    "@ai-sdk/openai": "^1.3.22",
    "@hookform/resolvers": "^5.0.1",
    "@inertiajs/inertia": "^0.11.1",
    "@inertiajs/react": "^2.0.11",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.49.9",
    "@vinejs/vine": "^3.0.1",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "edge.js": "^6.2.1",
    "lucide-react": "^0.511.0",
    "luxon": "^3.6.1",
    "pg": "^8.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "reflect-metadata": "^0.2.2",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.3.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.47"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config",
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
